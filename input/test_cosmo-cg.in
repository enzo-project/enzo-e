include "input/test_cosmo.incl"

 Adapt { max_level = 0; }
 Stopping { cycle = 200; }
 Method {
     gravity {
         solver = "cg";
     }
 }
 Solver {
     list = [ "cg" ];
     cg {
         iter_max = 100;
         monitor_iter = 10;
         res_tol = 0.01000000000000000;
         type = "cg";
     };
 }


 Output {
     de   { dir = [ "Dir_COSMO_CG_%04d", "cycle" ]; }
     depa { dir = [ "Dir_COSMO_CG_%04d", "cycle" ]; }
     ax   { dir = [ "Dir_COSMO_CG_%04d", "cycle" ]; }
     ay   { dir = [ "Dir_COSMO_CG_%04d", "cycle" ]; }
     az   { dir = [ "Dir_COSMO_CG_%04d", "cycle" ]; }
     dark { dir = [ "Dir_COSMO_CG_%04d", "cycle" ]; }
     mesh { dir = [ "Dir_COSMO_CG_%04d", "cycle" ]; }
     po   { dir = [ "Dir_COSMO_CG_%04d", "cycle" ]; }
     hdf5 { dir = [ "Dir_COSMO_CG_%04d", "cycle" ]; }
     dep  { dir = [ "Dir_COSMO_CG_%04d", "cycle" ]; }
     check { dir = [ "Dir_COSMO_CG_%04d-checkpoint", "count" ]; }
  }
