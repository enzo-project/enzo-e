include "input/ppm.incl"

Mesh { root_blocks    = [2,4]; }

Output {

  file_groups = ["density","checkpoint"];

  density {
     name = ["checkpoint_ppm-8-%06d.png", "cycle"];
  };

  checkpoint {

     type  = "checkpoint";
     dir   = ["checkpoint_ppm-8-%d","cycle"];
     schedule = ["cycle","list",100];
  }
}
