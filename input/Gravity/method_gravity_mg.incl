#----------------------------------------------------------------------
# Problem: 2D include file for EnzoMethodGravityMg test
# Author:  James Bordner (jobordner@ucsd.edu)
#----------------------------------------------------------------------
#
# This file initializes all but the following parameters, which must be initialized by
# the parameter file including this one:
#
#    Mesh : root_blocks
#    Output : phi_png : name
#    Output : rho_png   : name
#    Output : phi_h5  : name
#    Output : rho_h5    : name
#
#----------------------------------------------------------------------

Domain {
   lower = [ -1.0, -1.0 ];
   upper = [  1.0,  1.0 ];
}

 Adapt {
     list = [ "slope" ];
     slope {
         field_list = [ "density" ];
         max_coarsen = 0.010000000000000;
         min_refine = 4.0000000000000;
         type = "slope";
     }

     # Adapt:min_level needed for creating subblocks
     # Method:<gravity_mg>:min_level needed since it accesses subblocks
     min_level = -1;
 }

Mesh { 
   root_rank = 2;
}

Method {
    list = ["gravity_mg"]; 

    gravity_mg {
       min_level = -1;
       max_level = 0;
       type = "mg0";
       iter_max = 10;
    }

    ppm {
       diffusion   = true;
       flattening  = 3;
       steepening  = true;
       dual_energy = false;
   }
}

Field {
   
   list = ["density", "potential",
           "acceleration_x",
           "acceleration_y",
           "acceleration_z",
	   "total_energy",
           "velocity_x",
           "velocity_y",
           "velocity_z",
           "internal_energy",
	   "pressure",
           "B" ];

   ghost_depth = 4;
}

Monitor {
   verbose = true;
}

Initial {
   list = ["value"];

   value {
      density  = [ 1.0, (x - 0.825)*(x - 0.825) + 
   		     (y - 0.000)*(y - 0.000) < 0.003,
                   0.1 ];

      total_energy = [ 10.0 / (2.0/3.0 * 1.0),
                           (x - 0.825)*(x - 0.825) +
                           (y - 0.000)*(y - 0.000) < 0.003,
                        1.0 / (2.0/3.0 * 0.1) ];
      AP = 0.0;
      B  = 0.0;
      P  = 0.0;
      R  = 0.0;
      X  = 0.0;
   }
}

Boundary {
   type = "reflecting";
} 

Output {
   list = ["mesh_png", "phi_png", "rho_png", "ax_png", "ay_png",
           "B_png",
           "C_png",
           "D_png",
           "R_png",
           "X_png",
            "h5"];
    mesh_png {
	type     = "image";
	image_type  = "mesh";
	image_size = [257,257];
	image_reduce_type = "max";
        include "input/Schedule/schedule_cycle_1.incl"
	image_min = 0.0;
	image_max = 3.0;
        include "input/Colormap/colormap_rainbow.incl"
        axis = "z";
   }
   phi_png {
     type = "image";
     image_reduce_type = "max";
     image_size = [256,256];
     field_list = ["potential"];
     include "input/Schedule/schedule_cycle_1.incl"
     include "input/Colormap/colormap_blackbody.incl"
   }
   X_png {
     type = "image";
     image_reduce_type = "max";
     image_size = [256,256];
     field_list = ["X"];
     include "input/Schedule/schedule_cycle_1.incl"
     include "input/Colormap/colormap_blackbody.incl"
   }
   C_png {
     type = "image";
     image_reduce_type = "max";
     image_size = [256,256];
     field_list = ["C"];
     include "input/Schedule/schedule_cycle_1.incl"
     include "input/Colormap/colormap_blackbody.incl"
   }
   R_png {
     type = "image";
     image_reduce_type = "max";
     image_size = [256,256];
     field_list = ["R"];
     include "input/Schedule/schedule_cycle_1.incl"
     include "input/Colormap/colormap_blackbody.incl"
   }
   B_png {
     type = "image";
     image_reduce_type = "max";
     image_size = [256,256];
     field_list = ["B"];
     include "input/Schedule/schedule_cycle_1.incl"
     include "input/Colormap/colormap_blackbody.incl"
   }
   C_png {
     type = "image";
     image_reduce_type = "max";
     image_size = [256,256];
     field_list = ["C"];
     include "input/Schedule/schedule_cycle_1.incl"
     include "input/Colormap/colormap_blackbody.incl"
   }
   D_png {
     type = "image";
     image_reduce_type = "max";
     image_size = [256,256];
     field_list = ["D"];
     include "input/Schedule/schedule_cycle_1.incl"
     include "input/Colormap/colormap_blackbody.incl"
   }
   h5 {
     type = "data";
     include "input/Schedule/schedule_cycle_1.incl"
   }
   rho_png {
     type = "image";
     image_reduce_type = "max";
     image_size = [256,256];
     field_list = ["density"];
     include "input/Schedule/schedule_cycle_1.incl"
     include "input/Colormap/colormap_blackbody.incl"
   }
   ax_png {
     type = "image";
     image_reduce_type = "max";
     image_size = [256,256];
     field_list = ["acceleration_x"];
     include "input/Schedule/schedule_cycle_1.incl"
     include "input/Colormap/colormap_blackbody.incl"
   }
   ay_png {
     type = "image";
     image_reduce_type = "max";
     image_size = [256,256];
     field_list = ["acceleration_y"];
     include "input/Schedule/schedule_cycle_1.incl"
     include "input/Colormap/colormap_blackbody.incl"
   }
   az_png {
     type = "image";
     image_reduce_type = "max";
     image_size = [256,256];
     field_list = ["acceleration_z"];
     include "input/Schedule/schedule_cycle_1.incl"
     include "input/Colormap/colormap_blackbody.incl"
   }
   phi_h5 {
     type = "data";
     image_size = [256,256];
     field_list = ["potential"];
     include "input/Schedule/schedule_cycle_1.incl"
   }
   rho_h5 {
     type = "data";
     image_size = [256,256];
     field_list = ["density"];
     include "input/Schedule/schedule_cycle_1.incl"
   }
}

Stopping {
   cycle = 1;
}
