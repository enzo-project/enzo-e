# set images = (dots)
# set images = (dot)
# set images = (ucsd-seal-1024)
# set images = (gradient)
# set images = (norman)
# set images = (skory)
# set images = (sdsc-logo)
# set images = (cosmology-edit-2048)
# set images = (cosmo2)
# set images = (max2)
# set images = (saturn)
set images = (saturn2)

set color = gray

foreach image ($images)

    ln -sf ${image}.h image.h
    make dep
    make clean
    make
    test_tree
    echo "done"
    foreach tree (4 16)
        foreach type (0 1 2 3 4 5)
          h5topng -c $color tree$tree-$type.hdf5
          mv tree$tree-$type.hdf5.png $image-$tree-$type.png
        end
    end

end


